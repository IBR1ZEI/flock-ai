# Example environment variables for Flock-AI
# Flock App credentials
APP_ID=your-flock-app-id
APP_SECRET=your-flock-app-secret
PORT=80
BOT_TOKEN=your-flock-bot-token
BOT_USER_ID=your-flock-bot-user-id

# Letta AI API
LETTA_BASE_URL=your-letta-api-url
LETTA_AGENT_ID=your-letta-agent-id

# MongoDB
DB_USER=your-mongodb-username
DB_PASSWORD=your-mongodb-password
DATABASE_NAME=your-database-name
INSTALLED_USERS_COLLECTION=users

# Redis
REDIS_URL=your-redis-url

# Test user/channel (optional, for development/testing)
TEST_USER_ID=your-test-user-id
TEST_USER_TOKEN=your-test-user-token
TEST_CHANNEL_WEBHOOK_TOKEN=your-test-channel-webhook-token